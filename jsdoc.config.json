{
  "opts": {
    "template": "./node_modules/ink-docstrap/template",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "tutorials": "./tutorials"
  },
  "plugins": [
    "plugins/markdown",
    "plugins/summarize",
    "plugins/commentConvert",
    "plugins/overloadHelper",
    "plugins/underscore"
  ],
  "recurseDepth": 10,
  "source": {
    "include": ["./src/"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "script",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks"           : false,
    "monospaceLinks"        : true,
    "systemName"            : "Phaser 3 Webpack ES6 Project Template",
    "footer"                : "",
    "copyright"             : "Design1Online.com, LLC",
    "includeDate"           : "true",
    "navType"               : "vertical",
    "theme"                 : "united",
    "linenums"              : "true",
    "collapseSymbols"       : "true",
    "inverseNav"            : "false",
    "outputSourceFiles"     : "true" ,
    "outputSourcePath"      : "false",
    "dateFormat"            : "dddd, MMMM Do YYYY, h:mm:ss a",
    "syntaxTheme"           : "dark",
    "sort"                  : "true",
    "search"                : "true"
  }
}
